<Application
    x:Class="Karamem0.Kanpuchi.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:li="using:Karamem0.Kanpuchi.Interactivity">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/Generic.xaml" />
                <ResourceDictionary>
                    <li:ViewModelLocator x:Key="ViewModelLocator" />
                    <li:BooleanToVisibilityConveter x:Key="BooleanToVisibilityConveter" />
                    <li:ContentPageTypeConverter x:Key="ContentPageTypeConverter" />
                    <li:LocalTimeConverter x:Key="LocalTimeConverter" />
                    <li:StringToVisibilityConveter x:Key="StringToVisibilityConveter" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
